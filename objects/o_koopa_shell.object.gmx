    if (y &gt; other.y+3)
    {
        with (other) scr_player_bounceback();
        sound_ps(snd_squish,2,0);
        delay = 1;
        _fx = instance_create(other.x,self.y+8,o_fx_hit);
        _fx.image_speed = 0.5;
        hspeed = 0;
    }
    else
    {
        with (other) event_user(0);
    }
}
